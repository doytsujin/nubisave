/*
 * Dummy WSML service description for the Fortune service
 */

wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-flight"

namespace {
	_"http://localhost:8080/Matchmaker/ontologies/Rusco/Fortune.wsml#",
	qos _"http://localhost:8080/Matchmaker/ontologies/QoSBase.wsml#",
	context _"http://localhost:8080/Matchmaker/ontologies/ContextBase.wsml#",
	print _"http://localhost:8080/Matchmaker/ontologies/Printer2/PrinterContextQoSBase.wsml#",
	dc _"http://purl.org/dc/elements/1.1#",
	wsml _"http://www.wsmo.org/wsml/wsml-syntax#"
}

webService FortuneService
 importsOntology { _"http://localhost:8080/Matchmaker/ontologies/Printer2/PrinterContextQoSBase.wsml#" }

capability ServiceCapability
 postcondition definedBy ?serviceType memberOf print#PrintService.

interface Fortune
 importsOntology { BasicParam }

  nfp
    wsdl hasValue "http://example.org/services/FortuneService.wsdl"

    monitorendpoint hasValue "http://localhost:8060/axis/services/ConQoAccess"
    maasurl hasValue "http://localhost:8080/maasflotrwebapp/Chart.jsp"

    arrayno hasValue "1"
  endnfp

ontology BasicParam
 importsOntology { _"http://localhost:8080/Matchmaker/ontologies/Printer2/PrinterContextQoSBase.wsml#"}

 instance WsPriceInst1 memberOf { print#Price, qos#ServiceSpec}
  qos#value hasValue 1
  qos#unit hasValue qos#EUR

